package ZooSimulation;
public class BlackPanther extends Animal {
    @Override
    public void speak() {
        System.out.println("I'm an BlackPanther!");
    }
    @Override
    public void shout() {
        System.out.println("When I shout, I roar, roar, roar.");
    }
    @Override
    public void move() {
        System.out.println("When I move, I run, run, run.");
    }
}
